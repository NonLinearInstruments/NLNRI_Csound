<CsoundSynthesizer><CsInstruments>sr=44100kr=22050ksmps=2nchnls=2instr 1; Lotka Volterra model for predator-prey relations dynamics;;	dx/dt = Ax-Bxy;	dy/dt= -Cy+Dxy;;	parameters : A, B, C, D;;	Ignasi Alvarez, 1998;	Rev. 2005ka invalue "kparamA"kb invalue "kparamB"kc invalue "kparamC"kd invalue "kparamD"kdt invalue "kdtparam"kdt2 invalue "kdtparam"kgainx invalue "kgainx"kgainy invalue "kgainy"kdtportparam invalue "kdtportparam"ky init 1kx init 1kxo init 0kyo init 0kynew init 0kxnew init 0kynewo init 0kxnewo init 0kcont init 0ka = ka+p7kb = kb+p7kc = kc+p7kd = kd+p7kdt portk kdt,kdtportparamkdt2 portk kdt/2,kdtportparamkgainx portk kgainx,0.01kgainy portk kgainy,0.01loop:;sistema d'equacionskxnew=ka*kx-kb*kx*kykynew=(-1)*kc*ky+kd*kx*kykxo=kxkyo=kykxnewo=kxnewkynewo=kynewkx=kxo+kxnewo*kdtky=kyo+kynewo*kdt;sistema d'equacionskxnew=ka*kx-kb*kx*kykynew=(-1)*kc*ky+kd*kx*kykx=kxo+kdt2*(kxnew+kxnewo)ky=kyo+kdt2*(kynew+kynewo)kcont=kcont+1if kcont!=p4 kgoto loopkcont=0asig1 = kx*kgainx+ky*kgainy; Optionally output to a Display object:outvalue "kdisplax", 0.25+kx*kx/25outvalue "kdisplay", 0.25+ky/25outs asig1*p5,asig1*p6endin</CsInstruments><CsScore>f1	0	65536	10	1f0 120i1	0	3000	1	0.25	0.75	0i1	0	3000	1	0.5	0.5	0.01i1	0	3000	1	0.75	0.25	0.02e</CsScore></CsoundSynthesizer><MacOptions>Version: 3Render: RealAsk: YesFunctions: NoneWindowBounds: 7 44 1014 760Options: -b1024 -A -o/Users/ignasi/Documents/MAC CSOUND/mis cosas/Lorenz2.aif -s -m7 -R </MacOptions><MacGUI>ioView background {65535, 65535, 65535}
