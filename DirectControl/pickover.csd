<CsoundSynthesizer><CsInstruments>sr=44100kr=22050ksmps=2nchnls=2instr 1; Pickover attractor;;	x' = sin(ay) - z cos(bx);	y' = z sin(cx) - cos(dy);	z' = sin(x);;	parameters : a, b, c, d;;	Ignasi Alvarez, 1998;	Rev. 2005ka invalue "kparamA"kb invalue "kparamB"kc invalue "kparamC"kd invalue "kparamD"kdt invalue "kdtparam"kdt2 invalue "kdtparam"kgainx invalue "kgainx"kgainy invalue "kgainy"kgainz invalue "kgainz"kdtportparam invalue "kdtportparam"; les variables kfun es necessiten per calcular els valors amb l'opcode sin i coskfun1 init 0kfun2 init 0kfun3 init 0kfun4 init 0kpimedios = 1.570796326795ky init 1kz init 1kx init 1kxo init 0kyo init 0kzo init 0kynew init 0kxnew init 0kznew init 0kynewo init 0kxnewo init 0kznewo init 0kcont init 0kdt portk kdt,kdtportparamkdt2 portk kdt/2,kdtportparamkgainx portk kgainx,0.01kgainy portk kgainy,0.01kgainz portk kgainz,0.01loop:;sistema d'equacionskfun1 = sin(ka*ky*kpimedios)kfun2 = cos(kb*kx*kpimedios)kfun3 = sin(kc*kx*kpimedios)kfun4 = cos(kd*ky*kpimedios)kxnew=kfun1-kz*kfun2kynew=kfun3*kz-kfun4kznew= sin(kx)kxo=kxkyo=kykzo=kzkxnewo=kxnewkynewo=kynewkznewo=kznewkx=kxo+kxnewo*kdtky=kyo+kynewo*kdtkz=kzo+kznewo*kdt;sistema d'equacionskfun1 = sin(ka*ky*kpimedios)kfun2 = cos(kb*kx*kpimedios)kfun3 = sin(kc*kx*kpimedios)kfun4 = cos(kd*ky*kpimedios)kxnew=kfun1-kz*kfun2kynew=kfun3*kz-kfun4kznew= sin(kx)kx=kxo+kdt2*(kxnew+kxnewo)ky=kyo+kdt2*(kynew+kynewo)kz=kzo+kdt2*(kznew+kznewo)kcont=kcont+1if kcont!=p4 kgoto loopkcont=0asig1 = kx*kgainx+ky*kgainy+kgainz*kz; Optionally output to a Display object:outvalue "kdisplax", 1.25+kx/5outvalue "kdisplay", 1.25+ky/5outs asig1*p5,asig1*p6endin</CsInstruments><CsScore>f1	0	65536	10	1f0 120i1	0	300	1	1	1e</CsScore></CsoundSynthesizer><MacOptions>Version: 3Render: RealAsk: YesFunctions: NoneWindowBounds: 3 44 1269 845Options: -b1024 -A -o/Users/ignasi/Documents/M