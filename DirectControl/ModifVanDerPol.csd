<CsoundSynthesizer><CsInstruments>sr=44100kr=22050ksmps=2nchnls=2instr 1;====================================================================;; 	Shaun's Modified Van Der Pol non-perturbative forced oscillator.;	After E.Atlee Jackson, Perspectives on non-linear dynamics, Vol. I, 1991.; ;	x' = - y + A * cos(½t);	y' = x + µ*(1-x*x)*y;;; Ignasi Alvarez, 1998.; rev. 2005;; p4 = difrerencial de ½ per polifon’a.; p5 = "inversi— de fase" (+1, -1);;====================================================================kfreq invalue "komega"kgain invalue "kgain"kres invalue "kres"kfiltfreq invalue "kfiltfreq"kalfa init 2.328041kmu init 0.557286kcont init 0amod init 0ax init .5ay init .5axnew init 0aynew init 0afiltL init 0afiltR init 0aoutL init 0aoutR init 0kfreq = kfreq+p4kfiltreq portk kfiltfreq,0.01kgain portk kgain, 0.01kres portk kres,0.01amod phasor kfreq,0axnew = (-1)*ay+kalfa*amodaynew = ax+ay*kmu*(1-ax*ax)afiltL lowpass2 axnew*kgain,kfiltfreq,kresafiltR lowpass2 aynew*kgain,kfiltfreq,kresaoutL = afiltLaoutR = afiltRouts	aoutL*p5,aoutR*p5ax=axneway=aynew;Optionally output to a Display object:kax downsamp axkay downsamp ayoutvalue "kdisplay", 0.5+kax/5outvalue "kdisplax", 0.5+kay/5endin</CsInstruments><CsScore>f1	0	32768		10	1;redu•t a una crepitaci— ;i1	0	300	0	1;i1	0	300	0.00001	-1;la"constant" filtradai1	0	300	0	1i1	0	300	0.0001	-1; ... a "tutti plen";i1	0	300	0	1e</CsScore></CsoundSynthesizer><MacOptions>Version: 3Render: RealAsk: YesFunctions: NoneWindowBounds: 8 46 1248 839Options: -b512 -A -o/Users/ignasi/Documents/MAC CSOUND/mis cosas/ModifVanDerPol.aif -s -m7 -R </MacOptions><MacGUI>ioView background {65535, 65535, 65535}
