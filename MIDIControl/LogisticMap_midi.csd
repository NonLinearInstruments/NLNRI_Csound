<CsoundSynthesizer><CsInstruments>sr=44100kr=22050ksmps=2nchnls=2instr 1; "logistic map";kcx invalue "kcx";kcy invalue "kcx"kcx init 64kcy init 64kcxdet1 init 64ax init 0.1ay init 0.1axnew init 0aynew init 0kax init 0kay init 0kdifx init 0midicontrolchange 77,kcxmidicontrolchange 15,kcxdet1inotnum	notnumiamp	ampmidi	1kfreq invalue "kfreq"kband invalue "kband"kfreq2 invalue "kfreq2"kband2 invalue "kband2"kmaster invalue "kmaster"kmaster portk kmaster,0.1; centrat en la nota 64, kdif calcula un diferencial que va de -0.1 per la nota 0, a +0.1 per la nota 127kdifx = .1*(((inotnum/127)*2)-1)kcx = 3.57+(kcx*0.0025703125)+kcxdet1/12800-kdifxkcy = kcx+kdifxkampenv    linenr     iamp*32768,.1,.1,.01axnew=kcx*ax*(1-ax)aynew=kcy*ay*(1-ay)asig1L resonr axnew,kfreq,kbandasig1R resonr aynew,kfreq,kbandasig2L lowpass2 asig1L,kfreq2,kband2asig2R lowpass2 asig1R,kfreq2,kband2outs	asig2L*kmaster*kampenv,asig2R*kmaster*kampenv;Optionally output to a Display object:kax downsamp axkay downsamp ayoutvalue "kdisplay", kaxoutvalue "kdisplax", kayoutvalue "kdisplaxkcx", kcxoutvalue "kdisplaykcy", kcyax=axneway=aynewendin</CsInstruments><CsScore>f1	0	32768		10	1f0 600e</CsScore></CsoundSynthesizer><MacOptions>Version: 3Render: RealAsk: YesFunctions: NoneWindowBounds: 2 46 1279 810Options: -b128 -A -s -m7 -R </MacOptions><MacGUI>ioView background {65535, 65535, 65535}
