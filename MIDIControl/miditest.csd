<CsoundSynthesizer><CsInstruments>; M I D I ... !!!        sr = 44100        kr = 22050        ksmps = 2        nchnls = 2instr   1	kfreq init 127	kconstantfreq init 11025/(128*128)	icps	cpsmidi	iamp	ampmidi	0dbfs*.5	midicontrolchange 77,kfreq;generador (kampenv = declicker)	kampenv    linenr     iamp,.001,0.01,.001 	a1	pluck	kampenv,icps,icps,0,1;filtre		kfreq = 20+kfreq*kfreq*kconstantfreq 				a2 lowpass2 a1,kfreq,10	;sortida				outs	a2,a2endin</CsInstruments><CsScore>f1 0 512 10 1f0 600 e</CsScore></CsoundSynthesizer><MacOptions>Version: 3Render: RealAsk: YesFunctions: ioObjectWindowBounds: 10 100 510 655Options: -b64 -A -s -m7 -R </MacOptions><MacGUI>ioView nobackground {65535, 65535, 65535}
ioListing {10, 10} {350, 500}ioGraph {360, 10} {300, 500}</MacGUI>