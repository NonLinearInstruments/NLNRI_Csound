<CsoundSynthesizer><CsInstruments>sr=44100kr=44100ksmps=1nchnls=2;============================================;;	Ršssler Attractor;;============================================; 	;	Ignasi Alvarez - 1998;	rev.2005;;	________________________________________;	instr 1; rang de valors:; kc = 1.5 ~ 80; kdt = 0.0002 ~ 0.014ka init 0.2kb init 0.2knota init 0koct init 0kpitchconst init 1.059463094359 ; equal temperament kdtmin init 0.0002kdtmax init 0.014ky init 1kz init 1kx init 1kxo init 0kyo init 0kzo init 0kynew init 0kxnew init 0kznew init 0kynewo init 0kxnewo init 0kznewo init 0kc init 1.5kcfino init 0kportc init 0.0001kdt init 0kdt2 init 0kgainx init 0kgainy init 0kgainz init 0kampenv init 0kpanxL init 0kpanxR init 0kpanyL init 0kpanyR init 0kpanzL init 0kpanzR init 0kgainmaster init 1000; MIDI ==============================inotnum	notnumiamp	ampmidi	1koct = (inotnum-inotnum%12)/12knota = (inotnum%12); microKONTROL KORG default scene: ; sliders :midicontrolchange 77,kcmidicontrolchange 15,kportcmidicontrolchange 5,kgainxmidicontrolchange 20,kgainymidicontrolchange 82,kgainz;midicontrolchange 22,unnasigned;midicontrolchange 18,unnasigned;midicontrolchange 21,unnasigned; encoders:midicontrolchange 28,kcfino;midicontrolchange 29,unassignedmidicontrolchange 30,kpanxLmidicontrolchange 31,kpanyLmidicontrolchange 74,kpanzL;midicontrolchange 71,unnasigned;midicontrolchange 85,unnasigned;midicontrolchange 79,unnasigned; joystick Y;midicontrolchange 01,unnasigned;midicontrolchange 02,unnasigned;=====================================; calcul de Dtkdt = (kdtmin*kpitchconst^knota)*(2^koct) ;if (kdt>kdtmax) kgoto notamaximkgoto seguimnotamaxim:kdt=kdtmaxseguim:kdt2 = kdt/2;kdt portk kdt,kdtportparam;kdt2 portk kdt/2,kdtportparam; calcul de ckcfino = kcfino/128kc = kcfino+1.5+kc*.60546875 ;1.5 es el valor m’nim, el mˆxim es 79 (80 + 1 de kcfino) : d'aqu’ (79-1.5)/128=.60546875 : es a dir, linialment.kportc = .0001+kportc*.0781241 ; minim = 0.0001 , m‡xim = 10, rang 0-127 => 9.9999/128=.0781241kc portk kc,kportc,1.5; c‡lcul de guanyances i panorames :kgainx = (kgainx-64)/64kgainy = (kgainy-64)/64kgainz = (kgainz-64)/64kgainx portk kgainx,0.01kgainy portk kgainy,0.01kgainz portk kgainz,0.01kpanxL = kpanxL/128kpanxR = 1-kpanxLkpanyL = kpanyL/128kpanyR = 1-kpanyLkpanzL = kpanzL/128kpanzR = 1-kpanzL; optionally display values :outvalue "kdtparam", kdtoutvalue "kparamC", kcoutvalue "kportc", kportcoutvalue "kgainx", kgainxoutvalue "kgainy", kgainyoutvalue "kgainz", kgainzoutvalue "kpanxL", kpanxLoutvalue "kpanyL", kpanyLoutvalue "kpanzL", kpanzL; Sistema d'equacions===================kxnew=(-1)*(ky+kz)kynew=kx+ka*kykznew=kb+kx*kz-kc*kzkxo=kxkyo=kykzo=kzkxnewo=kxnewkynewo=kynewkznewo=kznewkx=kxo+kxnewo*kdtky=kyo+kynewo*kdtkz=kzo+kznewo*kdt;sistema d'equacionskxnew=(-1)*(ky+kz)kynew=kx+ka*kykznew=kb+kx*kz-kc*kzkx=kxo+kdt2*(kxnew+kxnewo)ky=kyo+kdt2*(kynew+kynewo)kz=kzo+kdt2*(kznew+kznewo);===========================================; Audio signals :asig1L = ky*kgainyasig2L = kx*kgainxasig3L = kz*kgainzasig1R = asig1Lasig2R = asig2Lasig3R = asig3Lkampenv    linenr     iamp*kgainmaster,.1,.1,.01outs kampenv*(asig1L*kpanxL+asig2L*kpanyL+asig3L*kpanzL),kampenv*(asig1R*kpanxR+asig2R*kpanyR+asig3R*kpanzR); Optionally output to a Display object:outvalue "kdisplay", 0.5+kx/250outvalue "kdisplax", 0.5-ky/250endin</CsInstruments><CsScore>f0 3000</CsScore></CsoundSynthesizer><MacOptions>Version: 3Render: RealAsk: YesFunctions: NoneWindowBounds: 2 46 1042 826Options: -b512 -A -o/Users/ignasi/Documents/MAC CSOUND/mis cosas/Lorenz2.aif -s -m7 -R </MacOptions><MacGUI>ioView background {65535, 65535, 65535}
